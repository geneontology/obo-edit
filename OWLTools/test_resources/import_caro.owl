<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE rdf:RDF [
    <!ENTITY owl 'http://www.w3.org/2002/07/owl#'>
    <!ENTITY rdf 'http://www.w3.org/1999/02/22-rdf-syntax-ns#'>
]>

<rdf:RDF
    xmlns:owl="&owl;"
    xmlns:rdf="&rdf;"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
>
<owl:Ontology rdf:about="http://example.org">
  <owl:imports rdf:resource="file:test_resources/caro.owl"/>
</owl:Ontology>

<owl:Class rdf:about="http://example.org/foo1">
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">foo</rdfs:label>
  <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/CARO_0000073"/>
</owl:Class>

</rdf:RDF>
