<!-- Start: nmatrix_results_2d.tmpl -->

<table border="1">
  <tr>

    <td></td>
    
    [% EO_C = 0 %]
    [% FOR term_acc IN axis_1_list %]
    [%   IF EO_C % 2 == 0 %]
    <td class="even_row">
    [%   ELSE %]
    <td class="odd_row">
    [%   END %]
      <span style="font-size: 80%;">
	[% term_info.$term_acc.name %]
      </span>
      <br />
      <span style="font-size: 67%; font-style: normal; font-weight: normal;">
	[% term_acc %]
      </span>
    </td>
    [%   EO_C = EO_C + 1 %]
    [% END %]
  </tr>
  
  <!-- -->
  [% EO_R = 0 %]
  [% FOR row_acc IN axis_2_list %]
  <tr>
    [% IF EO_R % 2 == 0 %]
    <td class="even_row">
    [% ELSE %]
    <td class="odd_row">
    [% END %]
      <span style="font-size: 80%;">
	[% term_info.$row_acc.name %]
	[% term_info.$row_acc.acc %]
      </span>
      <br />
      <span style="font-size: 67%; font-style: normal; font-weight: normal;">
	[% row_acc %]
      </span>
    </td>
    
    [% EO_C = 0 %]
    [% FOR col_acc IN axis_1_list %]
    
    [% IF EO_R % 2 == 0 AND EO_C % 2 == 0 %]
    <td class="crossed_row">
    [% ELSIF EO_R % 2 == 0 %]
    <td class="even_row">
    [% ELSIF EO_C % 2 == 0 %]
    <td class="even_row">
    [% ELSE %]
    <td class="odd_row">
      [% END %]
      [% IF matrix.$row_acc.$col_acc.gene_product_count > 0 %]
      <span title="Click to see additional information and options."
	    class="simulated-link"
	    style="text-decoration: underline;"
	    onclick="info_shield([ '[% row_acc %]', '[% col_acc %]' ]);">
	[% matrix.$row_acc.$col_acc.gene_products.size %]
      </span>
      [% ELSE %]
      0
      [% END %]
    </td>
    
    [%   EO_C = EO_C + 1 %]
    [% END %]
    
  </tr>
  
  [%   EO_R = EO_R + 1 %]
  [% END %]
  
</table>

<!-- End: nmatrix_results_2d.tmpl -->
