<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01//EN"
		"http://www.w3.org/TR/html4/strict.dtd">
<html lang="en">
	<head>
<meta http-equiv="content-type" content="text/html; charset=iso-8859-1">
	<title>AmiGO: Term Search Results</title>
		[% INCLUDE header_info.tmpl %]
		[%# INCLUDE javascript_term_search.tmpl %]
		<meta name="keywords" content="[% querylist.join(', ') %]">
	</head>
	<body>
[% INCLUDE header.tmpl %]
		<div class="contents">
			<h1>Term Search Results</h1>
[% TRY;
		INCLUDE query_summary.tmpl;
		IF data.size && data.size > 0;
			INCLUDE term_search_results.tmpl;
		END;
	CATCH;
		PROCESS error.tmpl;
	END; %]
		</div>
[% INCLUDE footer.tmpl %]
	</body>
</html>
