<!-- START: phylotree_cluster.tmpl -->

<img src="[% cluster.dist %]" width="100%" alt="[Cluster Distribution]" />

<div class="contents">

[% IF paint_comment %]<h3>[% paint_comment %]</h3>[% END %]
[% IF paint_files %]<ul>[% FOREACH file IN paint_files.keys %]
<ol><a href="[% paint_files.$file %]">[% file %]</a></ol>
[% END %]</ul>[% END %]

<dl class="filters">

<dt>1. Panthr Family ID</dt>
<dd>[% cluster.dbname %]::[% cluster.key %][% IF viz_link %]&nbsp;&nbsp;<a title="View the interactive tree page" href="[% viz_link %]"><img src=[% viz_img %] /></a>[% END %]</dd>

<dt>2. Date selected for concurrent annotation</dt>
<dd>Not in DB</dd>

<dt>3. Date Panthr family last annotated (tree annotation)</dt>
<dd>Not in DB</dd>

<dt>4. Number of members</dt>
<dd>[% cluster.number_of_members %]</dt>

<dt>5. Number Of RefG Members</dt>
<dd>[% cluster.number_of_refg_members %]</dd>

<dt>6. Number of members with EXP</dt>
<dd>[% cluster.exp %]</dd>

<dt>7. Date most recent members last annotated</dt>
<dd>[% cluster.last_annotated %]</dd>

<dt>8. 'Date comprehensively annotated' for groups that can provide this information</dt>
<dd>Not in DB</dd>

</dl>

<style type="text/css">th{text-align:left}</style>
<table><caption>Members</caption><tr>
<th>gene_product.symbol</th>
<th>dbxref</th>
<th colspan="2">species</th>
</tr>[% FOREACH gene_product IN gene_products %]<tr>
<td><a title="Go to gene product details page for [% gene_product.symbol %]" href="[% gene_product.link %]">[% gene_product.symbol %]</a></td>
<td>[% gene_product.dbxref %]</td>
<td[% IF gene_product.color %] style="color:[% gene_product.color %]">&#8251;[% ELSE %]>[% END %]</td>
<td>[% gene_product.species %]</td>
</tr>[% END %]</table>

<p><strong>&#8251;</strong> Reference Genome.</p>

</div>
<!-- END: phylotree_cluster.tmpl -->
