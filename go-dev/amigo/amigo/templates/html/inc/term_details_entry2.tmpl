<!-- START template: term_details_entry2.tmpl -->
<!-- graph_entry = [% graph_entry %] -->
<!-- number_of_spaces = [% number_of_spaces %] -->
<!-- row_number = [% row_number %] -->
<!-- override_image = [% override_image %] -->
<!-- override_title = [% override_title %] -->
[% rev = MAX_DISPLACEMENT - number_of_spaces %]
<!-- rev = [% rev %] -->
<tr style="padding: 0px; margin: 0px;">
 [% cacc = graph_entry.acc %]
 [% # "border-width: 1px; border-color: #666666; border-style: solid;" %]
 [% foo_nargle_mangle = 0; WHILE foo_nargle_mangle <= number_of_spaces; %]<td class="override" style="padding: 0px; margin: 0px; width: 1.5em; height: 1.0em;" id="addr-[% row_number %]-[% foo_nargle_mangle %]"></td>[% foo_nargle_mangle = foo_nargle_mangle + 1; END; %]
  <td colspan="[% rev %]" class="override" style="padding: 0px; margin: 0px;" id="grab-[% ACC_TO_RAND.$cacc %]">
  [% IF override_image && override_title %]
   <img src="[% override_image %]" title="[% override_title %]" />
  [% ELSE %]
  [%    IF graph_entry.inferred_p %]
   <img src="[% image_dir _ "/" _ graph_entry.rel _ "_inferred.gif" %]"
	title="Inferred [% graph_entry.rel %] relation to [% TERM_INFO.acc %]" />
  [%    ELSE %]
   <img src="[% image_dir _ "/" _ graph_entry.rel _ ".gif" %]"
	title="[% graph_entry.rel %]" />
  [%    END %]
  [% END %]
  [% IF override_link %]<span style="font-weight: bold;">[% graph_entry.acc %]&nbsp;[% graph_entry.name %]</span>[% ELSE %]<a href="[% graph_entry.link %]#lineage" title="Go to [% graph_entry.name %]">[% graph_entry.acc %]</a>&nbsp;[% graph_entry.name %][% END %][% for_acc = graph_entry.acc %]&nbsp;<span id="[% ACC_TO_RAND.$for_acc %]"></span>
</td>
</tr>
<!-- END template: term_details_entry2.tmpl -->
