        </serializedBean>
        <condition>!Util.isWindows()</condition>
        <validation />
        <actions />
        <formComponents>
          <formComponent name="" id="8731" beanClass="com.install4j.runtime.beans.formcomponents.MultilineLabelComponent" insetTop="" insetLeft="" insetBottom="" insetRight="" resetInitOnPrevious="false">
            <serializedBean>
              <java class="java.beans.XMLDecoder">
                <object class="com.install4j.runtime.beans.formcomponents.MultilineLabelComponent">
                  <void property="labelText">
                    <string>Because OBO-Edit2 is a Java application, it cannot use an arbitrary amount of system memory like a native application. Instead, you must set a fixed maximum amount of memory that OBO-Edit2 can use. Unless you have a 64-bit JVM, Java sets a limit of 1860M of allocated memory, even if you are using a computer with more memory. You can change the memory setting after installation via OBO-Edit's Configuration Manager or by hand-editing oboedit.vmoptions (located in the OBO-Edit installation directory).</string>
                  </void>
                </object>
              </java>
            </serializedBean>
            <initScript />
          </formComponent>
          <formComponent name="" id="8733" beanClass="com.install4j.runtime.beans.formcomponents.NumberSpinnerComponent" insetTop="" insetLeft="" insetBottom="" insetRight="" resetInitOnPrevious="false">
            <serializedBean>
              <java class="java.beans.XMLDecoder">
                <object class="com.install4j.runtime.beans.formcomponents.NumberSpinnerComponent">
                  <void property="initialValue">
                    <int>1024</int>
                  </void>
                  <void property="labelText">
                    <string>Maximum Memory for OBO-Edit2 (in MB)</string>
                  </void>
                  <void property="maxValue">
                    <int>1860</int>
                  </void>
                  <void property="minValue">
                    <int>256</int>
                  </void>
                  <void property="stepSize">
                    <int>128</int>
                  </void>
                  <void property="variableName">
                    <string>oboedit.memoryAlloc</string>
                  </void>
                </object>
              </java>
            </serializedBean>
            <initScript>Integer val;
val = new Integer((int) Math.max(1024,
(Math.round(SystemInfo.getPhysicalMemory() * 0.6 / 1024 / 1024) / 128) *128));
val = Math.min(val, 1860);
configurationObject.setValue(val);</initScript>
          </formComponent>
        </formComponents>
