<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.geneontology.gaf.hibernate.ExtensionExpression" table="extension_expression">
        <id name="id" type="string">
            <column name="id" />
            <generator class="assigned" />
        </id>
        <property name="cls" type="string">
            <column name="cls">
            </column>
        </property>

        <property name="relation" type="string">
            <column name="relation">
            </column>
        </property>


        </class>
 </hibernate-mapping>
