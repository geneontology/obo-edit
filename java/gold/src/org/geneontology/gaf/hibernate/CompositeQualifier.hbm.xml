<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.geneontology.gaf.hibernate.CompositeQualifier" table="composite_qualifier">
        <composite-id>
            <key-property name="id" type="string">
                <column name="id" />
            </key-property>
            <key-property name="qualifierObj" type="string">
                <column name="qualifier_obj" />
            </key-property>
        </composite-id>

    </class>
</hibernate-mapping>
