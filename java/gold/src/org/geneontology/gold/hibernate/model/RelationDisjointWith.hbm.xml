<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.geneontology.gold.hibernate.model.RelationDisjointWith" table="relation_disjoint_with">
        <composite-id>
        
	       <key-property name="ontology" column="ontology" type="string"/>
	       <key-property name="disjointRelation" column="disjoint_relation" type="string"/>
	       <key-property name="relation" column="relation" type="string"/>
         
        </composite-id>
    </class>
</hibernate-mapping>
