<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.geneontology.gold.hibernate.model.AnnotationAssertion" table="annotation_assertion">
         <composite-id>
	       <key-many-to-one name="ontology" class="org.geneontology.gold.hibernate.model.Ontology" column="ontology" />
	       <key-many-to-one name="relation" class="org.geneontology.gold.hibernate.model.Relation" column="relation" />
            <key-property name="obj" type="string">
                <column name="obj" />
            </key-property>
            <key-property name="targetObj" type="string">
                <column name="target_obj" />
            </key-property>
        </composite-id>
    </class>
</hibernate-mapping>
