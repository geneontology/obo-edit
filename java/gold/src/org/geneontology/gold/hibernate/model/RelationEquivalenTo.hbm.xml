<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.geneontology.gold.hibernate.model.RelationEquivalenTo" table="relation_equivalent_to">
        <composite-id>
        
	       <key-many-to-one name="ontology" class="org.geneontology.gold.hibernate.model.Ontology" column="ontology" />
	       <key-many-to-one name="equivalentRelation" class="org.geneontology.gold.hibernate.model.Relation" column="equivalent_relation" />
	       <key-many-to-one name="relation" class="org.geneontology.gold.hibernate.model.Relation" column="relation" />
         
        </composite-id>
    </class>
</hibernate-mapping>
