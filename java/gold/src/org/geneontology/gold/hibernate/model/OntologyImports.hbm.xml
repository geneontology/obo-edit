<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.geneontology.gold.hibernate.model.OntologyImports" table="ontology_imports">
        <composite-id>
	       <key-many-to-one name="ontology" class="org.geneontology.gold.hibernate.model.Ontology" column="ontology" />
            <key-property name="importsOntology" type="string">
                <column name="imports_ontology" />
            </key-property>
        </composite-id>
    </class>
</hibernate-mapping>
