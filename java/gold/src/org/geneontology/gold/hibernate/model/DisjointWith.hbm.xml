<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.geneontology.gold.hibernate.model.DisjointWith" table="disjoint_with">
      	<composite-id>
	       <key-property name="ontology" column="ontology" type="string"/>
	       <key-property name="disjointCls" column="disjoint_cls" type="string"/>
	       <key-property name="cls" column="cls" type="string"/>
    	</composite-id>
        
    </class>
</hibernate-mapping>