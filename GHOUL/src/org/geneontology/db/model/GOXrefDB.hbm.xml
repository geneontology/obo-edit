<?xml version="1.0"?>
	
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="org.gmod.db.GO.simpleObject.GOXrefDB" table="db">
		<id name="db_id" column="id" unsaved-value="undefined">
			<generator class="increment"/>
		</id>
		<property name="name" not-null="true"/>
		<property name="fullname"/>
		<property name="description" column="datatype"/>
		<property name="url" column="generic_url"/>
		<property name="url_syntax"/>
		<property name="url_example"/>
		<property name="urlprefix"/>
	</class>
</hibernate-mapping>
