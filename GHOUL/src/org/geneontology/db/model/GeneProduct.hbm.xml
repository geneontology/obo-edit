<?xml version="1.0"?>
	
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="org.geneontology.db.model.GeneProduct" table="gene_product">
	
		<id name="gp_id" column="id" unsaved-value="undefined">
			<generator class="increment"/>
		</id>
		
		<property name="symbol" not-null="true"/>
		<property name="full_name"/>
   		<many-to-one name="dbxref" column="dbxref_id" class="org.geneontology.db.model.DBXref"/>
  		<many-to-one name="species" column="species_id" class="org.geneontology.db.model.Species"/>
  		<many-to-one name="SO_type" column="type_id" class="org.geneontology.db.model.Term"/>

	</class>
</hibernate-mapping>