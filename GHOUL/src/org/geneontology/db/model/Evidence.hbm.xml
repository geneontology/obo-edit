<?xml version="1.0"?>
	
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="org.geneontology.db.model.Evidence" table="evidence">
	
		<id name="evidence_id" column="id" unsaved-value="undefined">
			<generator class="increment"/>
		</id>
		
		<property name="code" not-null="true"/>
    	<many-to-one name="association" column="association_id" class="org.geneontology.db.model.Association" not-null="true"/>
    	<many-to-one name="dbxref" column="dbxref_id" class="org.geneontology.db.model.DBXref" not-null="true"/>
   		<set name="withs" table="evidence_dbxref">
   		  <key column="evidence_id"/>
    	  <many-to-many column="dbxref_id" class="org.geneontology.db.model.DBXref"/>
		</set>

	</class>
</hibernate-mapping>